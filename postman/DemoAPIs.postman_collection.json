		{
			"name": "Register",
			"event": [
				{
					"listen": "test",
					"script": {
						"exec": [
							"// Eğer kullanıcı zaten varsa da başarılı say",
							"pm.test(\"Status code is 200 or 201 or 400 (already exists)\", function () {",
							"    pm.expect(pm.response.code).to.be.oneOf([200, 201, 400]);",
							"});",
							"",
							"pm.test(\"Response time is less than 3000ms\", function () {",
							"    pm.expect(pm.response.responseTime).to.be.below(3000);",
							"});",
							"",
							"// Eğer başarılıysa token kaydet",
							"if (pm.response.code === 201) {",
							"    pm.test(\"Response is successful\", function () {",
							"        var jsonData = pm.response.json();",
							"        pm.expect(jsonData.success).to.eql(true);",
							"    });",
							"",
							"    pm.test(\"User ID exists\", function () {",
							"        var jsonData = pm.response.json();",
							"        pm.expect(jsonData.data.id).to.be.a('number');",
							"    });",
							"",
							"    pm.test(\"Username exists\", function () {",
							"        var jsonData = pm.response.json();",
							"        pm.expect(jsonData.data.username).to.be.a('string');",
							"    });",
							"",
							"    pm.test(\"Token exists\", function () {",
							"        var jsonData = pm.response.json();",
							"        pm.expect(jsonData.data.token).to.be.a('string');",
							"    });",
							"",
							"    // Token'i kaydet",
							"    if (pm.response.json().data && pm.response.json().data.token) {",
							"        pm.environment.set(\"auth_token\", pm.response.json().data.token);",
							"    }",
							"} else if (pm.response.code === 400) {",
							"    // Kullanıcı zaten var, bu normal - login yapacağız",
							"    pm.test(\"User already exists (expected)\", function () {",
							"        pm.expect(true).to.be.true;",
							"    });",
							"}"
						],
						"type": "text/javascript",
						"packages": {},
						"requests": {}
					}
				}
			],
			"request": {
				"method": "POST",
				"header": [],
				"body": {
					"mode": "raw",
					"raw": "{\n  \"username\": \"volkan\",\n  \"password\": \"1234\"\n}\n",
